{
  "name": "andrew72ru/yii2-seo-tags",
  "description": "Add a description, keywords, etc. tags to site page by url",
  "type": "yii2-extension",
  "keywords": ["yii2","extension","seo"],
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "andrew72ru",
      "email": "andrew72ru@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "yiisoft/yii2": ">=2.0.10",
    "kartik-v/yii2-widget-typeahead": "^1.0",
    "kartik-v/yii2-widget-select2": "^2",
    "rmrevin/yii2-fontawesome": "~2.12",
    "intervention/image": "^2.3",
    "ext-imagick": "*",
    "electrolinux/phpquery": "^0.9.6"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*",

    "codeception/codeception": "^2.2",
    "codeception/c3": "*",
    "codeception/specify": "*",
    "codeception/verify": "*"
  },
  "autoload": {
    "psr-4": {
      "andrew72ru\\seotag\\": ""
    }
  },
  "extra": {
    "bootstrap": "andrew72ru\\seotag\\Bootstrap"
  }
}
